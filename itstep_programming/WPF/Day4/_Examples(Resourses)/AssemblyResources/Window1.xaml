<Window x:Class="AssemblyResources.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Ресурсы сборки" Height="374" Width="312">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
        </Grid.RowDefinitions>
        <Button Click="cmdPlay_Click"  Grid.Row="0" Padding="5">Next</Button>
        <Image Name="img" Grid.Row="1"  Source="Images/Blue hills.jpg"></Image>
        <Button Name="buttonGetResource"  Grid.Row="2"  Click="buttonGetResource_Click">Извлечь ресурс</Button>
        <Button Name="buttonGetAllResources" Grid.Row="3"  Click="buttonGetAllResources_Click">Информация о всех ресурсах</Button>
    </Grid>
</Window>
